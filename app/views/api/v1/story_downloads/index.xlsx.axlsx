# frozen_string_literal: true

wb = xlsx_package.workbook

wb.add_worksheet(name: 'Download') do |sheet|
  sheet.add_row ['Story', 'Downloaded at']

  @story_downloads.each do |download|
    sheet.add_row [download.story.title, download.created_at]
  end
end

wb.add_worksheet(name: 'Read') do |sheet|
  sheet.add_row ['Story', 'Read at']

  @story_reads.each do |read|
    sheet.add_row [read.story.title, read.created_at]
  end
end
